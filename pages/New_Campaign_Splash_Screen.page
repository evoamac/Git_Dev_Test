<apex:page standardController="Campaign" >
<script>
function load(){
window.open("/apex/CampaignWizard","_self");
}
function loadtemplate(){
window.open("/apex/CampaignTemplate","_self");
}
</script>
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockSection columns="1">
                <apex:commandButton value="Create New Campaign" oncomplete="load();" />
                <apex:commandButton value="Create Campaign From Template" oncomplete="loadtemplate();" />
            </apex:pageBlockSection>
        
        </apex:pageBlock>
    
    </apex:form>
</apex:page>